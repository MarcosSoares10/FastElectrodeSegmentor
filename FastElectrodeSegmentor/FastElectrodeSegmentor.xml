<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Segmentation</category>
  <title>FastElectrodeSegmentor</title>
  <description><![CDATA[This is a CLI module that can be bundled in an extension]]></description>
  <version>0.0.1</version>
  <documentation-url>http://www.example.com/Slicer/Modules/FastElectrodeSegmentor</documentation-url>
  <license>Slicer</license>
  <contributor>FirstName LastName (Institution), FirstName LastName (Institution)</contributor>
  <acknowledgements>This work was partially funded by NIH grant NXNNXXNNNNNN-NNXN</acknowledgements>
  <parameters>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>
    <integer>
      <name>threshold</name>
      <longflag>threshold</longflag>
     <flag>s</flag>
      <label>Threshold</label>
      <description><![CDATA[A integer value (in units of mm) passed to the algorithm]]></description>
      <default>1</default>
    </integer>
    <image>
      <name>inputVolume</name>
      <label>Input Volume</label>
      <channel>input</channel>
      <index>0</index>
      <description><![CDATA[Input volume]]></description>
    </image>
    <image type="label">
      <name>outputVolume</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <index>1</index>
      <description><![CDATA[Output Volume]]></description>
    </image>
    <directory>
      <name>datPath</name>
      <label>Dat Folder Path</label>
      <index>2</index>
    </directory>
  </parameters>
</executable>
